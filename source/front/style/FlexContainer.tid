title: $:/plugins/neuroforest/front/style/FlexContainer
type: text/css

div.flex-container {
  align-items: end;
  background-color: white;
  border-radius: 0.3em;
  display: flex;
  flex-wrap: wrap;
  padding: 0.5em 0em 1em 0em;
}

div.flex-container-item {
  flex: 1 1 20%;
  max-width: 20%;
  padding: 0em 0.5em 0em 0.5em;
  text-align: center;
  vertical-align: bottom;
}